class digimon1//PUNIMON
{
  color c1; // color cuerpo
  color c2; // color ojos 
  color c3; // color pupila 
  color c4; // color cuernos 
  {  
  c1 = (#FF0000);
  c2 = (#FFFFFF);
  c3 = (#000000);
  c4 = (#610B0B);  
  }
  
  
 void dibuja ()
 {
   fill(c4);// cuerno izquierdo 
    stroke (0);
    arc(100,256,50,120,radians(160),radians(340),OPEN);
  
    fill(c4);// cuerno derecho
    stroke (0);
    arc(245,289,50,200,radians(227),radians(356),OPEN);
  
   fill(c1);// cuerpo
   stroke(0);
   arc(170,300,200,200,radians(180), radians(360),CHORD);
  
    fill(c4); // cuerno central
    stroke (0);
    arc(180,240,50,120,radians(190),radians(360),OPEN);
    
    fill(c2); 
    ellipse(200,264,33,25);// ojo derecho
    fill(c3);
    ellipse(200,264,10,10);//pupila derecha
    fill(c2);
    noStroke();
    ellipse(202,261,2,2);//iluminacion derecha ojo
   
     stroke(0);
     fill(c2);
    ellipse(120,264,33,25);// ojo izquierdo
    fill(c3);
    ellipse(120,264,10,10);//pupila izquierda
    fill(c2);
    noStroke();
    ellipse(121,261,2,2);// iluminacion izquierda ojo  
 }

 } digimon1 A;



